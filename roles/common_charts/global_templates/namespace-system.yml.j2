apiVersion: v1
kind: Namespace
metadata:
  name: "{{ namespace_name }}"
  labels:
    systemNamespace: "true"
    tenantNamespace: "false"
    pod-security.kubernetes.io/enforce: "{{ pss_policy_level }}"
    pod-security.kubernetes.io/audit: "{{ pss_policy_level }}"
    pod-security.kubernetes.io/warn: "{{ pss_policy_level }}"
